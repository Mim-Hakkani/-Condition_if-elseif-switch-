#include<iostream>
#include<stdio.h>
using namespace std;
int main()
{
    int num;
    cout<<"Enter The Number : ";
    cin>>num;
    if(num%2==0)
     cout<<"The Number is Even Number : "<<num<<endl;
    else
    cout<<"The Number is odd Number : "<<num<<endl;


    return 0;



}
/*

input : 2
output:even

*/
