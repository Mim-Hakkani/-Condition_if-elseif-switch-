/*
50 50 60
Expected Output :
This is an isosceles triangle.
*/

#include<iostream>
#include<stdio.h>
using namespace std;
int main()
{
  int a,b,c;
  cout<<"Enter the triangle :";
  cin>>a>>b>>c;

  if(a==b && b==c)
    cout<<"This is an Equilateral triangle."<<endl;

  else if(a==b || a==c || b==c)
    cout<<"This is an Isosceles triangle."<<endl;

  else
    cout<<"This is an Scalene triangle."<<endl;

  return 0;

}
