#include<iostream>
#include<stdio.h>
using namespace std;
int main()
{
    int age;
    cout<<"Enter The Age : ";
    cin>>age;
    
    if(age>=0 && age<18)
         cout<<"You are not eligible for casting your vote"<<endl;
         
    else if(age<0)
        cout<<"Sorry You are not born."<<endl;
        
    else
       cout<<"Congratulation! You are eligible for casting your vote"<<endl;

    return 0;




}


/*

Test Data : 21
Expected Output :
Congratulation! You are eligible for casting your vote.

*/
