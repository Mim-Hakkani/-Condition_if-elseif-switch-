#include<iostream>
#include<stdio.h>
using namespace std;
int main()
{
   int number;
   cout<<"Enter the number : ";
   cin>>number;
   if(number==2 ||number==3)
    cout<<"This is prime number"<<endl;

   else if(number%2==0 ||number%3==0 ||number%4==0 ||number%5==0 ||number%6==0 ||number%7==0 ||number%8==0 ||number%9==0)
       {
           cout<<"not prmine";


       }
       else
       {
           cout<<"prime";



       }

   }


