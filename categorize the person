#include<iostream>
#include<stdio.h>
using namespace std;
int main()
{
   int height;
   cout<<"Enter the height of the person : ";
   cin>>height;

   if(height>=0 && height <=150)
    cout<<"The person is Dwarf."<<endl;

   else if(height>150 && height<=170)
    cout<<"The person is Medium"<<endl;

    else
    cout<<"The person is Tall"<<endl;

    return 0;




}

/*

Test Data : 135
Expected Output :
The person is Dwarf.

*/
